{
   "background": {
      "scripts": [ "js/jquery-3.3.1.min.js", "js/background.js", "js/popup.js" ]
   },
   "browser_action": {
      "default_icon": "20.png",
      "default_title": "RasperryCast remote",
      "default_popup": "popup.html"
   },
   "description": "Share videos to Rpi",
   "icons": {
      "128": "128.png",
      "16": "16.png",
      "48": "48.png"
   },
   "options_ui": {
     "page": "options.html"
   },

   "manifest_version": 2,
   "name": "RaspberryCast",
   "permissions": [ "contextMenus", "tabs", "notifications" ],
   "version": "1.4.0"
}
